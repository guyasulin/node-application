
<!DOCTYPE html>
<html>
    <% include ../views/tmp/header %>
<head>
  <title></title>
  <link rel='stylesheet' href='/stylesheets/style.css' />
</head>

<body>

  <h1><%= title %></h1>
   <h2>Photo application</h2>

  <div id="photos">
      <div class="row">
          <% photos.forEach(function(photo){ %>
          <div class="col-md-4 col-sm-6">
              <div class="panel panel-primary">
                  <div class="panel-heading">
                      <h3 class="panel-title"><%= photo.name %></h3>
                  </div>
                  <div class="panel-body">
                    <a href="/photo/<%=photo.id%>/download" >
                      <img src="/images/<%= photo.path %>" class="photo" >
                    </a>
                  </div>
              </div>
          </div>  
        
       <% }) %>
      </div>

  </div>

</div>


</body>

</html>